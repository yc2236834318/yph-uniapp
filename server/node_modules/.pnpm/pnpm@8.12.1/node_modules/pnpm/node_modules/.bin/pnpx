#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/web/web前端开发/07.uniapp/07.商城/server/node_modules/.pnpm/pnpm@8.12.1/node_modules/pnpm/bin/node_modules:/mnt/d/web/web前端开发/07.uniapp/07.商城/server/node_modules/.pnpm/pnpm@8.12.1/node_modules/pnpm/node_modules:/mnt/d/web/web前端开发/07.uniapp/07.商城/server/node_modules/.pnpm/pnpm@8.12.1/node_modules:/mnt/d/web/web前端开发/07.uniapp/07.商城/server/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/web/web前端开发/07.uniapp/07.商城/server/node_modules/.pnpm/pnpm@8.12.1/node_modules/pnpm/bin/node_modules:/mnt/d/web/web前端开发/07.uniapp/07.商城/server/node_modules/.pnpm/pnpm@8.12.1/node_modules/pnpm/node_modules:/mnt/d/web/web前端开发/07.uniapp/07.商城/server/node_modules/.pnpm/pnpm@8.12.1/node_modules:/mnt/d/web/web前端开发/07.uniapp/07.商城/server/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/pnpx.cjs" "$@"
else
  exec node  "$basedir/../../bin/pnpx.cjs" "$@"
fi
